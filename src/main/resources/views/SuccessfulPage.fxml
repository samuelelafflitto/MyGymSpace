<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.BorderPane?>
<?import javafx.scene.layout.StackPane?>
<?import javafx.scene.text.Font?>

<BorderPane prefHeight="600.0" prefWidth="800.0"
            xmlns="http://javafx.com/javafx/23" xmlns:fx="http://javafx.com/fxml/1"
            fx:controller="graphicalcontrollers.gui.SuccessfulPageController">

    <top>
        <fx:include source="TopBar.fxml" />
    </top>

    <center>
        <StackPane styleClass="background-pane">

            <Label text="Operation Success" styleClass="success-label">
                <font>
                    <Font name="System Bold" size="36.0" />
                </font>
            </Label>

        </StackPane>
    </center>

</BorderPane>